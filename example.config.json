{
  "excludeDirs": [
    ".git",
    "node_modules",
    ".svelte-kit",
    "build",
    "dist",
    "coverage"
  ],
  "includeFiles": [
    "*.svelte",
    "*.ts",
    "*.js"
  ],
  "maxFileSize": 524288,
  "minFileSize": 0,
  "includeGitInfo": false,
  "compressContent": true,
  "compressionThreshold": 1024
}